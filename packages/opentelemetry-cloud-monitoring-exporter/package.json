{
  "name": "@google-cloud/opentelemetry-cloud-monitoring-exporter",
  "version": "0.12.0",
  "description": "OpenTelemetry Google Cloud Monitoring Exporter allows the user to send collected metrics to Google Cloud Monitoring.",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": "GoogleCloudPlatform/opentelemetry-operations-js",
  "scripts": {
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json -p ../../",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "prepare": "npm run compile",
    "test": "nyc ts-mocha -p tsconfig.json 'test/**/*.test.ts'",
    "fix": "gts fix",
    "pretest": "npm run compile"
  },
  "keywords": [
    "opentelemetry",
    "nodejs",
    "Monitoring",
    "google-cloud-monitoring"
  ],
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.d.ts",
    "doc",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "devDependencies": {
    "@opentelemetry/api": "1.0.3",
    "@opentelemetry/api-metrics": "0.25.0",
    "@opentelemetry/core": "1.0.0",
    "@opentelemetry/resources": "1.0.0",
    "@opentelemetry/sdk-metrics-base": "0.25.0",
    "@types/mocha": "8.2.3",
    "@types/nock": "11.1.0",
    "@types/node": "14.17.19",
    "@types/sinon": "10.0.4",
    "codecov": "3.8.3",
    "gts": "3.1.0",
    "mocha": "8.4.0",
    "nock": "13.1.3",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "sinon": "11.1.2",
    "ts-mocha": "8.0.0",
    "typescript": "4.4.3"
  },
  "dependencies": {
    "@google-cloud/opentelemetry-resource-util": "^0.12.0",
    "@opentelemetry/semantic-conventions": "^1.0.0",
    "google-auth-library": "^7.0.0",
    "googleapis": "^87.0.0"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.0.0",
    "@opentelemetry/core": "^0.25.0 || ^1.0.0",
    "@opentelemetry/resources": "^0.25.0 || ^1.0.0",
    "@opentelemetry/sdk-metrics-base": "^0.25.0"
  }
}
