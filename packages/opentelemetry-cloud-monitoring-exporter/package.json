{
  "name": "@google-cloud/opentelemetry-cloud-monitoring-exporter",
  "version": "0.15.0",
  "description": "OpenTelemetry Google Cloud Monitoring Exporter allows the user to send collected metrics to Google Cloud Monitoring.",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": "GoogleCloudPlatform/opentelemetry-operations-js",
  "scripts": {
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json -p ../../",
    "lint": "gts lint",
    "clean": "gts clean",
    "precompile": "node ../../scripts/version-update.js",
    "compile": "tsc",
    "prepare": "npm run compile",
    "test": "nyc ts-mocha -p tsconfig.json 'test/**/*.test.ts'",
    "update-snapshot-tests": "SNAPSHOT_UPDATE=1 npm run test",
    "fix": "gts fix",
    "pretest": "npm run compile"
  },
  "keywords": [
    "opentelemetry",
    "nodejs",
    "Monitoring",
    "google-cloud-monitoring"
  ],
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.d.ts",
    "doc",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "config": {
    "snap-shot-it": {
      "sortSnapshots": true
    }
  },
  "devDependencies": {
    "@opentelemetry/api": "1.3.0",
    "@opentelemetry/core": "1.8.0",
    "@opentelemetry/resources": "1.8.0",
    "@opentelemetry/sdk-metrics": "1.8.0",
    "@types/mocha": "9.1.1",
    "@types/nock": "11.1.0",
    "@types/node": "14.18.34",
    "@types/sinon": "10.0.13",
    "codecov": "3.8.3",
    "gts": "3.1.1",
    "mocha": "9.2.2",
    "nock": "13.2.9",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "sinon": "14.0.2",
    "snap-shot-it": "7.9.7",
    "ts-mocha": "9.0.2",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "@google-cloud/opentelemetry-resource-util": "^1.2.0",
    "@google-cloud/precise-date": "^3.0.1",
    "@opentelemetry/semantic-conventions": "^1.0.0",
    "google-auth-library": "^7.0.0",
    "googleapis": "^97.0.0"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.0.0",
    "@opentelemetry/core": "^1.0.0",
    "@opentelemetry/resources": "^1.0.0",
    "@opentelemetry/sdk-metrics": "^1.0.0"
  }
}
