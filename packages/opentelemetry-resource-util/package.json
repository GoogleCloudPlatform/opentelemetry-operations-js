{
  "name": "@google-cloud/opentelemetry-resource-util",
  "version": "1.0.0",
  "description": "Resource util used by other @google-cloud/opentelemetry* packages",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": "GoogleCloudPlatform/opentelemetry-operations-js",
  "scripts": {
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json -p ../../",
    "lint": "gts lint",
    "clean": "gts clean",
    "precompile": "node ../../scripts/version-update.js",
    "compile": "tsc",
    "prepare": "npm run compile",
    "test": "nyc ts-mocha -p tsconfig.json 'test/**/*.test.ts'",
    "fix": "gts fix",
    "pretest": "npm run compile"
  },
  "keywords": [
    "opentelemetry",
    "nodejs",
    "Monitoring",
    "google-cloud-monitoring"
  ],
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.d.ts",
    "doc",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "devDependencies": {
    "@opentelemetry/api": "1.0.3",
    "@opentelemetry/resources": "1.0.0",
    "@opentelemetry/semantic-conventions": "1.0.0",
    "@types/mocha": "8.2.3",
    "@types/node": "14.17.21",
    "codecov": "3.8.3",
    "gts": "3.1.0",
    "mocha": "8.4.0",
    "nyc": "15.1.0",
    "ts-mocha": "8.0.0",
    "typescript": "4.4.4"
  },
  "peerDependencies": {
    "@opentelemetry/resources": "^1.0.0",
    "@opentelemetry/semantic-conventions": "^1.0.0"
  }
}
