{
  "name": "@google-cloud/opentelemetry-cloud-trace-propagator",
  "version": "0.20.0",
  "description": "OpenTelemetry propagation package for Google Cloud Trace format",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": "GoogleCloudPlatform/opentelemetry-operations-js",
  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean",
    "precompile": "version-update.js",
    "compile": "tsc",
    "prepack": "npm run compile",
    "test": "nyc ts-mocha -p tsconfig.json 'test/**/*.test.ts'",
    "fix": "gts fix",
    "pretest": "npm run compile"
  },
  "keywords": [
    "opentelemetry",
    "nodejs",
    "tracing",
    "google-cloud-trace"
  ],
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.d.ts",
    "doc",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "devDependencies": {
    "@google-cloud/ops-js-scripts": "^1.0.0",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/core": "2.0.0",
    "@types/mocha": "10.0.8",
    "@types/node": "14.18.63",
    "gts": "5.3.1",
    "mocha": "10.7.3",
    "nyc": "15.1.0",
    "ts-mocha": "10.0.0",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "hex2dec": "^1.0.1"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.0.0",
    "@opentelemetry/core": "^2.0.0"
  }
}
