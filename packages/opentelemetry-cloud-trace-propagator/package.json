{
  "name": "@google-cloud/opentelemetry-cloud-trace-propagator",
  "version": "0.4.1",
  "description": "OpenTelemetry propagation package for Google Cloud Trace format",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": "GoogleCloudPlatform/opentelemetry-operations-js",
  "scripts": {
    "lint": "gts check",
    "lint:fix": "gts fix",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json -p ../../",
    "clean": "rimraf build/*",
    "compile": "tsc -p .",
    "prepare": "npm run compile",
    "test": "nyc ts-mocha -p tsconfig.json 'test/**/*.test.ts'"
  },
  "keywords": [
    "opentelemetry",
    "nodejs",
    "tracing",
    "google-cloud-trace"
  ],
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.d.ts",
    "doc",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "devDependencies": {
    "@types/mocha": "7.0.2",
    "@types/node": "14.0.14",
    "codecov": "3.7.1",
    "gts": "1.0.0",
    "mocha": "7.2.0",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "ts-mocha": "7.0.0",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "@opentelemetry/api": "^0.10.0",
    "@opentelemetry/core": "^0.10.0",
    "hex2dec": "^1.0.1"
  }
}
