{
  "name": "@google-cloud/opentelemetry-cloud-trace-propagator",
  "version": "0.8.0",
  "description": "OpenTelemetry propagation package for Google Cloud Trace format",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": "GoogleCloudPlatform/opentelemetry-operations-js",
  "scripts": {
    "lint": "gts lint",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json -p ../../",
    "clean": "gts clean",
    "compile": "tsc",
    "prepare": "npm run compile",
    "test": "nyc ts-mocha -p tsconfig.json 'test/**/*.test.ts'",
    "fix": "gts fix",
    "pretest": "npm run compile"
  },
  "keywords": [
    "opentelemetry",
    "nodejs",
    "tracing",
    "google-cloud-trace"
  ],
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.d.ts",
    "doc",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "devDependencies": {
    "@opentelemetry/api": "^0.16.0",
    "@opentelemetry/core": "^0.16.0",
    "@types/mocha": "7.0.2",
    "@types/node": "14.14.28",
    "codecov": "3.8.1",
    "gts": "3.1.0",
    "mocha": "7.2.0",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "ts-mocha": "8.0.0",
    "typescript": "4.1.5"
  },
  "dependencies": {
    "hex2dec": "^1.0.1"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^0.16.0",
    "@opentelemetry/core": "^0.16.0"
  }
}
