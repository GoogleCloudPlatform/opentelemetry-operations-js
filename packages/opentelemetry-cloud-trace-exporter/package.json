{
  "name": "@google-cloud/opentelemetry-cloud-trace-exporter",
  "version": "2.1.0",
  "description": "OpenTelemetry Google Cloud Trace Exporter allows the user to send collected traces to Google Cloud Trace.",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": "GoogleCloudPlatform/opentelemetry-operations-js",
  "scripts": {
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json -p ../../",
    "lint": "gts lint",
    "clean": "gts clean",
    "precompile": "version-update.js",
    "compile": "tsc",
    "prepack": "npm run compile",
    "test": "nyc ts-mocha -p tsconfig.json 'test/**/*.test.ts'",
    "fix": "gts fix",
    "pretest": "npm run compile"
  },
  "keywords": [
    "opentelemetry",
    "nodejs",
    "tracing",
    "google-cloud-trace"
  ],
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.d.ts",
    "doc",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "devDependencies": {
    "@google-cloud/ops-js-scripts": "^1.0.0",
    "@opentelemetry/api": "1.6.0",
    "@opentelemetry/core": "1.17.1",
    "@opentelemetry/resources": "1.17.1",
    "@opentelemetry/sdk-trace-base": "1.17.1",
    "@types/mocha": "10.0.3",
    "@types/node": "14.18.63",
    "@types/sinon": "10.0.19",
    "codecov": "3.8.3",
    "gcp-metadata": "6.0.0",
    "gts": "5.2.0",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "rimraf": "5.0.5",
    "sinon": "17.0.0",
    "ts-mocha": "10.0.0",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@google-cloud/opentelemetry-resource-util": "^2.1.0",
    "@grpc/grpc-js": "^1.1.8",
    "@grpc/proto-loader": "^0.7.0",
    "google-auth-library": "^9.0.0",
    "google-proto-files": "^4.0.0"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.0.0",
    "@opentelemetry/core": "^1.0.0",
    "@opentelemetry/resources": "^1.0.0",
    "@opentelemetry/sdk-trace-base": "^1.0.0"
  }
}
